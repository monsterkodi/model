#!/usr/bin/env bash
echo rm app
rm -rf app
rm -rf Strudl.app
echo gulp app
gulp app
echo mv app
mv app/Strudl.app .
echo link modules
ln -s ../../../../node_modules ./Strudl.app/Contents/Resources/app/node_modules
echo link js
rm -rf ./Strudl.app/Contents/Resources/app/js
ln -s ../../../../js ./Strudl.app/Contents/Resources/app/js
echo clean
rm -rf app
echo done
